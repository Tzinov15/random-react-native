{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "rules": {
    "interface-name": [
      false
    ],
    "object-literal-sort-keys": [
      false
    ],
    "member-ordering": [
      false
    ],
    "ordered-imports": [
      false
    ],
    "no-empty-interface": false,
    "member-access": false,
    "max-line-length": true,
    "no-console": true,
    "variable-name": false,
    "no-empty": false
  }
}